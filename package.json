{
  "name": "inserttimestamp",
  "displayName": "InsertTimestamp",
  "description": "Insert custom formatted date and time to current editor.",
  "version": "0.0.1",
  "publisher": "zvlad",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:inserttimestamp.perform"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "inserttimestamp.perform",
        "title": "Insert Time Stamp"
      }
    ],
    "keybindings": [
      {
        "command": "inserttimestamp.perform",
        "key": "ctrl+f5",
        "mac": "cmd+f5",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "moment-timezone": "^0.5.5"
  }
}